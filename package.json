{
  "name": "hlab-test-backend",
  "private": true,
  "version": "0.0.1",
  "description": "Nest TypeScript starter repository",
  "license": "Kantapat Testing - HLAB",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^18.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/lodash": "^4.17.6",
    "axios": "^1.6.7",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis": "^2.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.11",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "mssql": "^10.0.2",
    "mysql2": "^3.9.2",
    "nestjs-typeorm-paginate": "^4.0.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/src$1",
      "^@repositories(.*)$": "<rootDir>/src/repositories$1",
      "^@entities(.*)$": "<rootDir>/src/entities$1",
      "^@commons(.*)$": "<rootDir>/src/commons$1",
      "^@configs(.*)$": "<rootDir>/src/configs$1",
      "^@modules(.*)$": "<rootDir>/src/modules$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@exceptions(.*)$": "<rootDir>/src/exceptions$1",
      "^@shared(.*)$": "<rootDir>/src/shared$1",
      "^@interfaces(.*)$": "<rootDir>/src/interfaces$1",
      "^@responses(.*)$": "<rootDir>/src/commons/responses$1",
      "^@requests(.*)$": "<rootDir>/src/commons/requests$1",
      "^@enums(.*)$": "<rootDir>/src/commons/enums$1",
      "^@interceptors(.*)$": "<rootDir>/src/interceptors$1",
      "^@resources(.*)$": "<rootDir>/src/commons/resources$1",
      "^@validations(.*)$": "<rootDir>/src/commons/validations$1",
      "^@middlewares(.*)$": "<rootDir>/src/middlewares$1"
    }
  }
}
